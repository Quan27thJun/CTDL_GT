#include <bits/stdc++.h>
using namespace std;
int main() {
    int T;
    cin >> T;
    while (T--) {
        int n,x;
        cin >> n>>x;
        int a[n];
        int count=0;
        for (int i = 0; i < n; i++) {
            cin >> a[i];
        }
        for(int i=0;i<n;i++){
            if(a[i]==x){
                count=1;
                break;
            } else {
                count=0;
            }
        }
        if(count==1){
            cout<<"1"<<endl;
        } else {
            cout<<"-1"<<endl;
        }
    }
    return 0;
}
