#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int n, i, j, k, K, dem = 0;
        cin >> n >> K;
        long long a[n];
        for (i = 0; i < n; i++)
        {
            cin >> a[i];
        }
        sort(a, a + n);
        for (i = 0; i < n - 2; i++)
        {
            int j = i + 1, k = n - 1;
            while (j < k)
            {
                if (a[k] + a[j] + a[i] < K)
                {
                    dem += k - j;
                    j++;
                }
                else
                {
                    k--;
                }
            }
        }
        cout << dem << endl;
    }
}
